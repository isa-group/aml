<met:metricXML xmlns:met="http:/www.isa.us.es/ada/metrics">
    <met:metric id="Cost" type="integer" min="1" max="100"/>
	<met:metric id="Time" type="integer" min="1" max="100"/>
	<met:metric id="Size" type="integer" min="1" max="100"/>
	<met:metric id="Percent" type="integer" min="1" max="100"/>
	<met:metric id="Languages" type="enumerated">
        <met:value value="ES"/>
        <met:value value="EN-UK"/>
        <met:value value="FR"/>
		<met:value value="Auto"/>
	</met:metric>
	<met:metric id="Boolean" type="enumerated">
        <met:value value="True"/>
        <met:value value="False"/>
    </met:metric>
</met:metricXML>
