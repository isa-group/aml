<?xml version="1.0" encoding="UTF-8"?>
<wsag:AgreementOffer wsag:AgreementId="1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:wsag="http://schemas.ggf.org/graap/2007/03/ws-agreement"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xsi:schemaLocation="http://schemas.ggf.org/graap/2007/03/wsagreement agreement_types.xsd http://www.w3.org/2001/XMLSchema XMLSchema.xsd">
	<wsag:Name>TwoOptions</wsag:Name>
	<wsag:Context>
	 <wsag:AgreementInitiator>
       I need translation Corp. 
     </wsag:AgreementInitiator>   
     <wsag:AgreementResponder>
       Translator 
     </wsag:AgreementResponder>   
     <wsag:ServiceProvider>
       AgreementResponder 
     </wsag:ServiceProvider>   
     <wsag:ExpirationTime>
       2012-01-01T00:00:00
     </wsag:ExpirationTime>
	 <wsag:TemplateId>1.1</wsag:TemplateId>
	 <wsag:TemplateName>TranslateIt</wsag:TemplateName>
	</wsag:Context>
	<wsag:Terms wsag:Name="TranslationService">
		<wsag:All>
			<wsag:ServiceProperties wsag:Name="SP1" 
			wsag:ServiceName="TranslationService"> 
				<wsag:VariableSet> 
					<wsag:Variable wsag:Name="TranslationTime" wsag:Metric="metrics/TranslationServiceMetric:Time"> 
						<wsag:Location> 
							/TranslationTime
						</wsag:Location>
					</wsag:Variable>
					<wsag:Variable wsag:Name="InputErrors" wsag:Metric="metrics/TranslationServiceMetric:Percent"> 
						<wsag:Location> 
							/InputErrors
						</wsag:Location>
					</wsag:Variable>
				</wsag:VariableSet> 
			</wsag:ServiceProperties>
			<wsag:ServiceDescriptionTerm wsag:Name="SDT1" 
			wsag:ServiceName="TranslationService">
				<OfferItem name="SourceLang" wsag:Metric="metrics/TranslationServiceMetric:Languages">ES</OfferItem> 
				<OfferItem name="TargetLang" wsag:Metric="metrics/TranslationServiceMetric:Languages">FR</OfferItem>
				<OfferItem name="InputFileSize" wsag:Metric="metrics/TranslationServiceMetric:Size">20</OfferItem> 
				<OfferItem name="Cost" wsag:Metric="metrics/TranslationServiceMetric:Cost">20</OfferItem>  <!-- in cents of euros -->
				<OfferItem name="ImageTranslation" wsag:Metric="metrics/TranslationServiceMetric:Boolean"></OfferItem> 
			</wsag:ServiceDescriptionTerm>
			<wsag:GuaranteeTerm wsag:Name="GTNotTranslationTime1" wsag:Obligated="ServiceProvider" >
				<wsag:ServiceLevelObjective>
					<wsag:CustomServiceLevel>
						ImageTranslation=="False" AND NOT TranslationTime&lt;=1
					</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Name="GTTranslationTime2" wsag:Obligated="ServiceProvider" >
				<wsag:ServiceLevelObjective>
					<wsag:CustomServiceLevel>
						ImageTranslation=="True" AND TranslationTime&lt;=2
					</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
			<wsag:GuaranteeTerm wsag:Name="GTNotInputErrors" wsag:Obligated="ServiceConsumer" >
				<wsag:ServiceLevelObjective>
					<wsag:CustomServiceLevel>
						NOT InputErrors&lt;=1 
					</wsag:CustomServiceLevel>
				</wsag:ServiceLevelObjective>
			</wsag:GuaranteeTerm>
		</wsag:All>
	</wsag:Terms>
</wsag:AgreementOffer>